{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      // "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    }
  },
  "isort.interpreter": ["python3", "-m", "isort"],
  "python.sortImports.args": ["--settings-path", "./pyproject.toml"],

  "isort.args": ["--profile", "black", "--settings-path", "./pyproject.toml"],
  "cSpell.words": [
    "autenticacion",
    "Autenticacion",
    "boton",
    "coleccion",
    "devolucion",
    "Devolucion",
    "Dificil",
    "fisica",
    "Fisicos",
    "generico",
    "guia",
    "guias",
    "LANCZOS",
    "liquidacion",
    "metodo",
    "mshops",
    "notificacion",
    "Notificacion",
    "opcion",
    "seleccion",
    "sqlalchemy",
    "Tecnica",
    "ultimaid",
    "Vacio",
    "Vacios",
    "validacion"
  ]
}
